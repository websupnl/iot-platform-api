generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id        String   @id @default(cuid())
  name      String
  slug      String   @unique
  apiKey    String   @unique
  isActive  Boolean  @default(true)
  createdAt DateTime @default(now())
}
